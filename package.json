{
  "name": "airdrop-room",
  "version": "1.1.0",
  "description": "APP new generation",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently npm:watch:*",
    "watch:login": "postcss tailwindcss src/css/input.css -o dist/public/Styles/login.css --watch",
    "watch:register": "postcss tailwindcss src/css/input.css -o dist/public/Styles/register.css --watch",
    "watch:index": "postcss tailwindcss src/css/input.css -o dist/public/Styles/index.css --watch",
    "watch:post": "postcss tailwindcss src/css/input.css -o dist/public/Styles/post.css --watch",
    "watch:profil": "postcss tailwindcss src/css/input.css -o dist/public/Styles/profil.css --watch",
    "watch:posting": "postcss tailwindcss src/css/input.css -o dist/public/Styles/posting.css --watch",
    "watch:profilPublic": "postcss tailwindcss src/css/input.css -o dist/public/Styles/profilPublic.css --watch",
    "watch:beranda": "postcss tailwindcss src/css/input.css -o dist/public/Styles/beranda.css --watch",
    "watch:editProfile": "postcss tailwindcss src/css/input.css -o dist/public/Styles/profilPublicSession.css --watch",
    "build": "cross-env NODE_ENV=production postcss src/css/input.css -o dist/css/style.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.1.7"
  },
  "dependencies": {
    "daisyui": "^2.20.0",
    "flowbite": "^1.5.2"
  }
}